KoboDeluxe_bundle: ${EXEFILE}
	mkdir -p $(bundle_contents)/MacOS
	echo "APPL????" > $(bundle_contents)/PkgInfo
	$(INSTALL_DATA) Info.plist $(bundle_contents)
	$(INSTALL_PROGRAM) ${EXEFILE} $(bundle_contents)/MacOS/

install-exec-hook: KoboDeluxe_bundle
	rm -rf $(DESTDIR)$(prefix)/KoboDeluxe.app
	mkdir -p $(DESTDIR)$(prefix)/
	cp -r KoboDeluxe.app $(DESTDIR)$(prefix)/

clean-local:
	rm -rf $(bundle_contents)

